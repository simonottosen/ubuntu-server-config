[
  {
    "id": "deploy",
    "execute-command": "/scripts/deploy.sh",
    "response-message": "Executing deploy webhook ...",
    "pass-arguments-to-command": [
      {
        "source": "payload",
        "name": "repository.full_name"
      }
    ],
    "trigger-rule": {
      "match": {
        "type": "value",
        "value": "simonottosen/ubuntu-server-config",
        "parameter": {
          "source": "payload",
          "name": "repository.full_name"
        }
      }
    }
  }
]